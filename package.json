    "next": "^13.4.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "server-only": "^0.0.1",
    "tailwind-variants": "^0.1.5",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
