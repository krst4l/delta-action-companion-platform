    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.13.0",
    "stylelint": "^14.16.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.3.2"
